<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@id/web_chart_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/margin_20dp"
        android:layout_marginTop="@dimen/margin_20dp"
        android:layout_marginRight="@dimen/margin_20dp"
        android:layout_marginBottom="@dimen/margin_20dp" />

    <LinearLayout
        android:id="@id/order_bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="@dimen/ordersheet_20dp"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/ordersheet_20dp">

        <TextView
            android:id="@id/print_cosume_order"
            android:layout_width="@dimen/ordersheet_120dp"
            android:layout_height="@dimen/ordersheet_32dp"
            android:background="@drawable/member_search_edit_bg"
            android:gravity="center"
            android:text="打印消费单"
            android:textColor="@color/ordersheet_remark_color"
            android:textSize="@dimen/ordersheet_14sp" />

        <TextView
            android:id="@id/fan_jie_zhang_tv"
            android:layout_width="@dimen/ordersheet_120dp"
            android:layout_height="@dimen/ordersheet_32dp"
            android:layout_marginLeft="@dimen/ordersheet_20dp"
            android:background="@drawable/member_search_edit_bg"
            android:gravity="center"
            android:text="反结账"
            android:textColor="@color/ordersheet_remark_color"
            android:textSize="@dimen/ordersheet_14sp" />

        <TextView
            android:id="@id/back_to_list"
            android:layout_width="@dimen/ordersheet_120dp"
            android:layout_height="@dimen/ordersheet_32dp"
            android:layout_marginLeft="@dimen/ordersheet_20dp"
            android:background="@drawable/member_search_edit_bg"
            android:gravity="center"
            android:text="返回列表"
            android:textColor="@color/ordersheet_remark_color"
            android:textSize="@dimen/ordersheet_14sp" />

    </LinearLayout>
</RelativeLayout>